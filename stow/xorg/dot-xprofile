/usr/bin/env xrdb ~/.Xresources
/usr/bin/env feh --bg-scale ~/dotfiles/conf/wallpaper/wallhaven-453lo1.jpg
/usr/bin/env xss-lock ~/dotfiles/scripts/lock.sh&
# Set compose key (for unicode) to menu:
/usr/bin/env setxkbmap -option compose:menu
xmodmap -e "keycode 113 = Super_L"  # reassign Alt_R to Super_L
xmodmap -e "remove mod1 = Super_L"  # make sure X keeps it out of the mod1 group
# /usr/bin/env kitty -T=ncspot --session="$HOME/dotfiles/conf/kitty/sessions/ncspot"&
