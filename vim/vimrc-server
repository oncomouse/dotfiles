let &runtimepath .= ','.expand('~/dotfiles/vim/')
let &runtimepath .= ','.expand('~/dotfiles/vim/after/')
 call plug#begin()
runtime bundle/common.vim
runtime bundle/vim.vim
call plug#end()
runtime common.vim
let configs = [
  \ "fuzzy",
  \ "git",
  \ "statusline",
  \ "syntax",
  \ "theme",
\]
for file in configs
  let x = "config/".file.".vim"
  execute 'runtime' x
endfor

let g:nerdfonts=0

runtime after.vim
" # vim:foldmethod=marker
