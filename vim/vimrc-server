let &runtimepath .= ','.expand('~/dotfiles/vim/')
call plug#begin()
runtime bundle/common.vim
runtime bundle/vim.vim
call plug#end()
runtime common.vim
let configs = [
  \ "deoplete",
  \ "fuzzy",
  \ "git",
  \ "syntax",
  \ "theme",
\]
for file in configs
  let x = "config/".file.".vim"
  execute 'runtime' x
  endfor
runtime after.vim
